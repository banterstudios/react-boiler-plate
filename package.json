{
  "name": "react-boiler-plate",
  "version": "1.0.0",
  "description": "Personal boiler plate",
  "main": "index.js",
  "directories": {
    "test": "test"
  },
  "scripts": {
    "production": "webpack -p --config webpack.prod.config.js",
    "start": "webpack-dev-server",
    "dev": "webpack -d --config webpack.dev.config.js",
    "standardise": "standard --fix",
    "test": "mocha --watch --compilers js:babel-core/register --require ignore-styles ./test/utils/dom.js --recursive ./test/*/*.spec.js"
  },
  "author": "Nick de rozarieux",
  "license": "ISC",
  "devDependencies": {
    "babel-core": "^6.22.1",
    "babel-loader": "^6.2.10",
    "babel-preset-es2015": "^6.22.0",
    "babel-preset-react": "^6.22.0",
    "babel-preset-stage-0": "^6.22.0",
    "chai": "^3.5.0",
    "cheerio": "^0.22.0",
    "css-loader": "^0.26.1",
    "enzyme": "^2.7.1",
    "file-loader": "^0.10.0",
    "html-webpack-plugin": "^2.28.0",
    "ignore-styles": "^5.0.1",
    "jsdom": "^9.9.1",
    "mocha": "^3.2.0",
    "node-sass": "^4.4.0",
    "react": "^15.4.2",
    "react-addons-test-utils": "^15.4.2",
    "react-dom": "^15.4.2",
    "sass-loader": "^4.1.1",
    "sinon": "^1.17.7",
    "standard": "^8.6.0",
    "standard-loader": "^5.0.0",
    "style-loader": "^0.13.1",
    "webpack": "^1.14.0",
    "webpack-dev-server": "^1.16.2"
  },
  "dependencies": {
    "compass-mixins": "^0.12.10",
    "react": "^15.4.2",
    "react-dom": "^15.4.2",
    "react-redux": "^5.0.2",
    "redux": "^3.6.0",
    "redux-logger": "^2.7.4",
    "redux-thunk": "^2.2.0"
  }
}
